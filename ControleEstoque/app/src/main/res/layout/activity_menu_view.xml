<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.MenuView">

    <ImageView
        android:id="@+id/imgAdd"
        android:layout_width="@dimen/img_largura"
        android:layout_height="@dimen/img_largura"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        app:layout_constraintEnd_toStartOf="@+id/imgRemove"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/receive_novo" />

    <ImageView
        android:id="@+id/imgRemove"
        android:layout_width="@dimen/img_largura"
        android:layout_height="@dimen/img_largura"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgAdd"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/donation" />

    <ImageView
        android:id="@+id/imgStorage"
        android:layout_width="@dimen/img_largura"
        android:layout_height="@dimen/img_largura"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="112dp"
        android:layout_marginBottom="115dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgAdd"
        app:srcCompat="@drawable/estoque" />

    <ImageView
        android:id="@+id/imgDate"
        android:layout_width="@dimen/img_largura"
        android:layout_height="@dimen/img_largura"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginTop="117dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginBottom="110dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgStorage"
        app:layout_constraintTop_toBottomOf="@+id/imgRemove"
        app:srcCompat="@drawable/calendar_ok_green" />
</androidx.constraintlayout.widget.ConstraintLayout>